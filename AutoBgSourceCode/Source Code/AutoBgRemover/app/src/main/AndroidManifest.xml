<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="mobo.example.autobgremover">

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id"/>

        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/AppTheme.NoActionBar" android:name=".MainActivity"></activity>
        <activity android:theme="@style/AppTheme.NoActionBar" android:name=".MyCreationActivity"></activity>
        <activity android:theme="@style/AppTheme.NoActionBar" android:name="com.example.samplestickerapp.stickermaker.erase.erase.ShareActivity" />
        <activity android:theme="@style/AppTheme.NoActionBar" android:name="com.example.samplestickerapp.stickermaker.erase.erase.EraserActivity" />
        <activity android:theme="@style/AppTheme.NoActionBar" android:name="com.example.samplestickerapp.stickermaker.erase.erase.AdvanceEditActivity" />
        <activity android:name=".CreateCollageActivity" android:screenOrientation="portrait"></activity>



    </application>

</manifest>